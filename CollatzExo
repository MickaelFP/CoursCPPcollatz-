// Example program
#include <iostream>
#include <string>
#include <vector>

using namespace std

int collatz(n > 0)
{
    if(n == 1)
    {
        cout << "n = 1" << endl;
        cout << "Magique!" << endl;
        return 0;
    }
    else if( n % 2 == 0)
    {
        n = n/2;
        collatz n;
    }
    else{
        n = 3n + 1;
        collatz n;
    }
}

